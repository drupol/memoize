suites:
  tagattributes:
    namespace: drupol\Memoize
    psr4_prefix: drupol\Memoize

formatter.name: pretty

extensions:
  LeanPHP\PhpSpec\CodeCoverage\CodeCoverageExtension:
    format:
      - html
      - clover
      - php
      - text
    output:
      html: build/coverage
      clover: build/coverage.xml
      php: build/coverage.php